{
  "name": "tomodomo/blocks",
  "description": "Custom blocks for WordPress",
  "type": "wordpress-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Tomodomo",
      "email": "hello@tomodomo.co"
    }
  ],
  "config": {
    "vendor-dir": "build/composer",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-name": "tomodomo-blocks"
  },
  "repositories": [],
  "require": {
    "php": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "Tomodomo\\Blocks\\": [
        "src/blocks/"
      ],
      "Tomodomo\\BlockLib\\": [
        "src/lib/"
      ]
    }
  }
}
